{
  "name": "emoji-console-log",
  "displayName": "Emoji Console Log",
  "description": "Inserts console.log() statements with a random emoji",
  "version": "2.10.4",
  "publisher": "sapegin",
  "engines": {
    "vscode": "^1.50.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Emoji Console Log Configuration",
      "properties": {
        "turboConsoleLog.insertEmptyLineBeforeLogMessage": {
          "type": "boolean",
          "default": false,
          "description": "Whether to insert an empty line before the log message."
        },
        "turboConsoleLog.insertEmptyLineAfterLogMessage": {
          "type": "boolean",
          "default": true,
          "description": "Whether to insert an empty line after the log message."
        },
        "turboConsoleLog.logFunction": {
          "type": "string",
          "default": "console.log",
          "description": "Function to log the values."
        }
      }
    },
    "commands": [
      {
        "command": "turboConsoleLog.displayLogMessage",
        "title": "Turbo Console Log: Display Log Message"
      },
      {
        "command": "turboConsoleLog.commentAllLogMessages",
        "title": "Turbo Console Log: Comment All Log Messages"
      },
      {
        "command": "turboConsoleLog.uncommentAllLogMessages",
        "title": "Turbo Console Log: Uncomment All Log Messages"
      },
      {
        "command": "turboConsoleLog.deleteAllLogMessages",
        "title": "Turbo Console Log: Delete All Log Messages"
      }
    ],
    "keybindings": [
      {
        "command": "turboConsoleLog.displayLogMessage",
        "key": "ctrl+alt+l",
        "mac": "ctrl+alt+l"
      },
      {
        "command": "turboConsoleLog.commentAllLogMessages",
        "key": "alt+shift+c",
        "mac": "alt+shift+c"
      },
      {
        "command": "turboConsoleLog.uncommentAllLogMessages",
        "key": "alt+shift+u",
        "mac": "alt+shift+u"
      },
      {
        "command": "turboConsoleLog.deleteAllLogMessages",
        "key": "alt+shift+d",
        "mac": "alt+shift+d"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rm -rf out && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --external:prettier --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "npm run esbuild-watch",
    "lint": "eslint . --ext .js,.ts,.tsx --cache --fix",
    "format": "prettier --log-level warn --write \"**/*.{js,ts,tsx,md}\"",
    "pretest": "yarn run esbuild-base",
    "test-compile": "rm -rf out && tsc -p ./ && mkdir out/test/files && cp -r src/test/files/ out/test/files/",
    "test": "npm run test-compile && node ./out/test/runTests.js",
    "test:unit": "npm run test-compile && mocha --require ts-node/register 'src/test/unit/**/*.test.ts'",
    "prepare": "husky"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "chai": "^4.3.7",
    "chalk": "^4.1.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-tamia": "^8.1.3",
    "glob": "^7.1.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^8.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapegin/emoji-console-log/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sapegin/emoji-console-log"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": "prettier --write",
    "*.{js,ts,tsx}": "eslint --cache --fix"
  }
}
